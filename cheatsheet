#! /bin/sh

movetoscreen()
{
  echo "
  To move an already running process to screen:

	  1. Suspend the process-----------------------Ctrl+Z
	  2. Resume the process in the background----------bg
	  3. Disown the process---------------------disown %1
	  4. Launch a screen session-------------------screen
	  5. Find the PID of the process----------------pgrep
	  6. Use reptyr to take over the process----reptyr -s

  "
}

awkcolumn()
{
  echo "
  awk '{print \$number}'

    "
}

