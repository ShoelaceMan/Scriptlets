unblockip()
{
  echo "echo -e "IP?"; read IP; grep $IP /var/log/lfd.log /usr/local/cpanel/logs/cphulkd.log /etc/csf/csf.* /etc/hosts.allow; grep "deny" /etc/hosts.allow | grep -v "#"; /usr/local/cpanel/scripts/cphulkdwhitelist $IP; csf -a $IP ; csf -tr $IP; whmapi1 flush_cphulk_login_history_for_ips ip=$IP"
}

hackedphp()
{
  echo "
  find /home/*/public_html/ -ctime -100 -type f -iname "*.php" -exec awk 'FNR==1 && /eval/ || /strlen/ || /strto/ || /auth_pass/ || /_dl/ || /GLOBALS/  { print FILENAME  }; FNR>1 {nextfile}' {} +
    "
}

$1
